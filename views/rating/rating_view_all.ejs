<!DOCTYPE html>
<html>
<head>
    <title>Rating - View All</title>
</head>
<body>
<h1>All Book Ratings</h1>

<table border="1">
    <tr>
        <th>Title</th>
        <th>Rating</th>
        <th>User</th>
    </tr>
    <% for(var i = 0; i < ratings.length; i++) { %>
    <tr>
        <td><%= ratings[i].title %></td>
        <td><%= ratings[i].rating %></td>
        <td><%= ratings[i].username %></td>
        <td>
            <a href="/rating/edit/?user_id=<%= ratings[i].user_id %>&?book_id=<%=ratings[i].book_id%>">
                Edit
            </a>
        </td>
    </tr>
    <% } %>
</table>
<br />
<a href="http://localhost:3000/">Home</a>
</body>
</html>