<!DOCTYPE html>
<html>
<head>
    <title>User - View All</title>
</head>
<body>
<h1>All Users</h1>

<table border="1">
<tr>
    <th>ID</th>
    <th>Username</th>
    <th>Actions</th>
</tr>
    <% for(var i = 0; i < users.length; i++) { %>
    <tr>
        <td><%= users[i].user_id %></td>
        <td><%= users[i].username %></td>
        <td>
            <a href="/user/edit/?user_id=<%= users[i].user_id %>">
                Edit
            </a>
            <a href="/user/delete/?user_id=<%= users[i].user_id %>" >
                Delete
            </a>
        </td>
    </tr>
    <% } %>
</table>
<br />
<a href="http://localhost:3000/">Home</a>
</body>
</html>